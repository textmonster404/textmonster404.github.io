<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="style.xsl"?>
<title>Josh's Travel Journal</title>

<!--

Syntax:

<entry>
    <date></date>
    <description></description>
    <event>
        <time></time>
        <info>
            <note></note>
        </info>
    </event>
</entry>


Multiple <event>/<note> tags are allowed.
-->
<entry>
    <date>7-22-2018</date>
    <description>My road trip with Grandpa and Yvonne.</description>
    <event>
        <time>~10:00</time>
        <info>
            <note>
                <note_value>Left to go to peach festival.</note_value>
            </note>
            <note>
                <note_value>Went on Highway 70 to Highway 20.</note_value>
            </note>
            <note>
                <note_value>Went 2 miles east on Highway 20 to Marysville.</note_value>
            </note>
            <note>
                <note_value>Stopped at the peach festival.</note_value>
            </note>
            <note>
                <note_value>Went on 20 to 99 then forked onto 113.</note_value>
            </note>
            <note>
                <note_value>We traveled the entire length of 113 for the first time.</note_value>
            </note>
            <note>
                <note_value>Went down 12 to Rio Vista and ate at the head restaurant.</note_value>
            </note>
            <note>
                <note_value>Went on the road arcoss the river from 160 back home.</note_value>
            </note>
            <note>
                <note_value>Arrived at 3:00</note_value>
            </note>
        </info>
    </event>
</entry>
